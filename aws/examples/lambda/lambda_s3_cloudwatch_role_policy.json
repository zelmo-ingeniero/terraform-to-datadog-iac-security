{
  "Version": "2012-10-17",
   "Statement": [
    {
      "Effect": "Allow",
      "Resource": ["arn:aws:logs:${}::"],
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ]
    },
    {
      "Effect": "Allow",
      "Resource": ["arn:aws:s3:::remote-backend:infra"],
      "Action": [
        "s3:PutObject",
        "s3:GetObject"
      ]
    }
  ]
}
